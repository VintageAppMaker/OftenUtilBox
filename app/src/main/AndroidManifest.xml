<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.test.psw.oftenutilbox">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OftenUtilBox"
        android:usesCleartextTraffic="true">
        <activity
            android:name="oftenutilbox.viam.psw.example.activity.WebViewActivity"
            android:exported="true" />
        <activity
            android:name="oftenutilbox.viam.psw.example.activity.ViewPagerTestActivity"
            android:exported="true" />
        <activity android:name="oftenutilbox.viam.psw.example.activity.NestedScrollCustomActivity" />
        <activity android:name="oftenutilbox.viam.psw.example.activity.QuickExampleActivity" />
        <activity android:name="oftenutilbox.viam.psw.example.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>